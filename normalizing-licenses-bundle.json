// Augment the transformations at https://github.com/jk1/Gradle-License-Report/blob/master/src/main/resources/spdx-license-normalizer-bundle.json
{
  "transformationRules" : [
    { "bundleName" : "CDDL-1.0", "licenseNamePattern" : "CDDL + GPLv2 with classpath exception" }, // Since it's an OR, e normalize this to CDDL (1.0) instead, since the version of CDDL is not specified
    { "bundleName" : "CDDL-1.0", "licenseNamePattern" : "CDDL+GPL License" },                      // Since it's an OR, e normalize this to CDDL (1.0) instead, since the version of CDDL is not specified
    { "bundleName" : "EPL-1.0", "licenseNamePattern" : "MPL 2.0 or EPL 1.0" },                     // Since it's an OR, we chose EPL since it's already reviewed and allowed
    { "bundleName" : "CDDL-1.0", "licenseNamePattern" : "CDDL.*1\\.0" },                                                      // Remove on v3.0.2+ - Missing rules from parent bundle
    { "bundleName" : "CDDL-1.1", "licenseNamePattern" : "CDDL.*1\\.1" },                                                      // Remove on v3.0.2+ - Missing rules from parent bundle
    { "bundleName" : "LGPL-2.1-only", "licenseNamePattern" : ".*GNU LESSER GENERAL PUBLIC LICENSE.*2\\.1.*" },                // Remove on v3.0.2+ - Missing rules from parent bundle
    { "bundleName" : "LGPL-3.0-only", "licenseNamePattern" : ".*GNU LESSER GENERAL PUBLIC LICENSE.*3.*" },                    // Remove on v3.0.2+ - Missing rules from parent bundle
    { "bundleName" : "MIT", "licenseNamePattern" : "(The\\s)?Bouncy Castle Licen[cs]e", "transformUrl" : false },             // Remove on v3.0.2+ - Missing rules from parent bundle
    { "bundleName" : "MIT-0", "licenseNamePattern" : "(The\\s)?MIT No Attribution(\\s[l|L]icen[c|s]e)?(\\s[(]MIT[-]?0[)])?" } // Remove on v3.0.2+ - Missing rules from parent bundle
  ]
}
